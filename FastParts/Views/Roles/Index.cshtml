@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "Roles";
}
<h2>Roles</h2>
<p>@Html.ActionLink("Crear Rol", "Create", null, new { @class = "btn btn-primary" })</p>
<table class="table table-striped">
    <tr><th>Nombre</th><th></th></tr>
    @foreach (var r in Model)
    {
        <tr>
            <td>@r.Name</td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = r.Id }, new { @class = "btn btn-sm btn-warning" })
                @Html.ActionLink("Eliminar", "Delete", new { id = r.Id }, new { @class = "btn btn-sm btn-danger", style = "margin-left:6px" })
            </td>
        </tr>
    }
</table>
