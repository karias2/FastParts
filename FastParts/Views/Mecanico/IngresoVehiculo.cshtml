@{
    ViewBag.Title = "Ingreso de Vehículo";
}

<h2>Ingreso de Vehículo</h2>

<form style="max-width: 500px; margin-top: 20px;">
    <div class="form-group">
        <label>Marca del vehículo</label>
        <input type="text" class="form-control" placeholder="Ej. Toyota" />
    </div>

    <div class="form-group">
        <label>Modelo</label>
        <input type="text" class="form-control" placeholder="Ej. Hilux" />
    </div>

    <div class="form-group">
        <label>Placa</label>
        <input type="text" class="form-control" placeholder="Ej. ABC123" />
    </div>

    <div class="form-group">
        <label>Año</label>
        <input type="number" class="form-control" placeholder="Ej. 2022" />
    </div>

    <button type="submit" class="btn btn-primary mt-3">Registrar Vehículo</button>
</form>
